<launch>
  <arg name="port" default="8080" />
  <arg name="addr" default="127.0.0.1" />

  <node pkg="spinal_ros_bridge" type="udp_socket_node" name="rosserial_server" output="screen">
    <param name="server_port" value="$(arg port)" />
    <param name="client_port" value="$(arg port)" />
    <param name="client_addr" value="$(arg addr)" />
  </node>

  <node pkg="spinal_ros_bridge" type="rosservice_server_manager.py" name="" output="screen" />
  <node pkg="rosserial_python" type="message_info_service.py" name="rosserial_message_info" output="screen"/>

</launch>
