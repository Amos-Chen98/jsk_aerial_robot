<?xml version="1.0"?>
<launch>
  <arg name="robot_ns" default="hydrus" />

  <node pkg="hydrus" type="tilted_transform_control_node" name="lqi_gain_generator"  output="screen" ns="$(arg robot_ns)">
    <rosparam file="$(find hydrus)/config/quad/default_mode_201907/LQI.yaml" command="load" />
    <param name="tf_prefix" value="$(arg robot_ns)" />
    <param name="kinematic_verbose" value="false" />
    <param name="control_verbose" value="false" />
    <param name="debug_verbose" value="false" />
    <param name="verbose" value="false"/>
  </node>

</launch>
